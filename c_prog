//This is my first program-document section
#include <stdio.h> //preprocessor directory/header file
int main()
{
    int var1,var2;
    printf("Enter the first number :");
    scanf("%d",&var1);
    printf("Enter the second number :");
    scanf("%d",&var2);
    printf("Multiplication : %d\n",var1*var2);
    printf("Subtraction : %d\n",var2-var1);
    printf("Addition : %d\n",var1+var2);
    printf("Division : %d\n",var1/var2);
    return 0;
}

